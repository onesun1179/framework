{

  "compilerOptions": {
    "typeRoots": [
      "./src/types/global.d.ts"
    ],
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "strict": true,
    "paths": {
      "@common/*" : ["./src/common/*"],
      "@common" : ["./src/common"],
      "@auth/*" : ["./src/auth/*"],
      "@auth" : ["./src/auth"],
      "@file/*" : ["./src/file/*"],
      "@file" : ["./src/file"],
      "@util" : ["./src/common/util"],
      "@modules/code/*" : ["./src/module/code/*"],
      "@modules/code" : ["./src/module/code"],
      "@modules/app-metadata/*" : ["./src/module/app-metadata/*"],
      "@modules/app-metadata" : ["./src/module/app-metadata"],
      "@modules/front-component/*" : ["./src/module/front-component/*"],
      "@modules/front-component" : ["./src/module/front-component"],
      "@modules/icon/*" : ["./src/module/icon/*"],
      "@modules/icon" : ["./src/module/icon"],
      "@modules/menu/*" : ["./src/module/menu/*"],
      "@modules/menu" : ["./src/module/menu"],
      "@modules/message/*" : ["./src/module/message/*"],
      "@modules/message" : ["./src/module/message"],
      "@modules/role/*" : ["./src/module/role/*"],
      "@modules/role" : ["./src/module/role"],
      "@modules/route/*" : ["./src/module/route/*"],
      "@modules/route" : ["./src/module/route"],
      "@modules/user/*" : ["./src/module/user/*"],
      "@modules/user" : ["./src/module/user"]
    },
    "incremental": true,
    "skipLibCheck": true
  }
}
